<h1>New retain_user</h1>

<%= error_messages_for :retain_user %>

<% form_for(@retain_user) do |f| %>
  <p>
    <b>Retid</b><br />
    <%= f.text_field :retid %>
  </p>

  <p>
    <b>Password</b><br />
    <%= f.text_field :password %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', retain_users_path %>
