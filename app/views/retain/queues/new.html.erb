<h1>New queue</h1>

<%= error_messages_for :queue %>

<% form_for(@queue) do |f| %>
  <p>
    <b>Queue name</b><br />
    <%= f.text_field :queue_name %>
  </p>

  <p>
    <b>Center</b><br />
    <%= f.text_field :center %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', queues_path %>
