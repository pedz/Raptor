<h1>Create a favorite Retain queue</h1>

<%= error_messages_for :queue %>

<% form_for(@favorite_queue) do |f| %>
  <p>
    <b>Queue name</b><br />
    <%= f.text_field :queue_name %>
  </p>

  <p>
    <b>Center</b><br />
    <%= f.text_field :center %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Index of Favorite Queues', favorite_queues_path %>
