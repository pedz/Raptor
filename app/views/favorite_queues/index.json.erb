<%# -*- coding: utf-8 -*- %>

<%= @favorite_queues.map do |favorite_queue|
      queue = favorite_queue.queue.wrap_with_combined
      {
        :combined_q_url => combined_q_path(queue.to_param),
        :queue => queue.to_param,
        :team => queue.owners.empty?,
        :hits => queue.hits(:json)
      }
    end.to_json
%>
