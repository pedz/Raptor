<h1>Listing team_memberships</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Membership type</th>
    <th>User</th>
  </tr>

<% @team_memberships.each do |team_membership| %>
  <tr>
    <td><%=h team_membership.team_id %></td>
    <td><%=h team_membership.membership_type_id %></td>
    <td><%=h team_membership.user_id %></td>
    <td><%= link_to 'Show', team_membership %></td>
    <td><%= link_to 'Edit', edit_team_membership_path(team_membership) %></td>
    <td><%= link_to 'Destroy', team_membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team_membership', new_team_membership_path %>