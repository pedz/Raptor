<%# -*- coding: utf-8 -*- %>

<h1>New feedback_note</h1>

<%= error_messages_for :feedback_note %>

<% form_for([@feedback, @feedback_note]) do |f| %>
  <p>
    <b>Note</b><br />
    <%= f.text_area :note, :size => "75x20" %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', @feedback %>
