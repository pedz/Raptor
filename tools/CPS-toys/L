                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "news";
                    mySearchForm.url.value = "";
                    mySearchForm.expandAdv.value = "no";
                    break;
                }
                else if ((theValue == "news") && (isAdvance == "yes")) 
                {
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[1];
                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "news";
                    mySearchForm.expandAdv.value = "yes";
                    mySearchForm.url.value = "";
                    break;
                }
                else if ((theValue == "assets") && (isAdvance == "no")) 
                { 
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[0];
                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "assets";
                    mySearchForm.url.value = "";
                    mySearchForm.expandAdv.value = "no";
                    break;
