	function ns_7_B0ODECD418JK30II4MBA0M30I0_isEmpty(myText) 
	{
		var re = /^\s{1,}$/g; //match any white space including space, tab, form-feed, etc.

		if ((myText==null) || (myText.length==0) || ((myText.search(re)) > -1)) 
		{
			return true;
		}
		else 
		{
			return false;
		}
	}


	function ns_7_B0ODECD418JK30II4MBA0M30I0_isFieldEmpty(myField) 
	{
		var re = /^\s{1,}$/g; //match any white space including space, tab, form-feed, etc.

		if ((myField.value==null) || (myField.value.length==0) || ((myField.value.search(re)) > -1)) 
		{
			return true;
		}
		else 
		{
			return false;
		}
	}

    function ns_7_B0ODECD418JK30II4MBA0M30I0_searchSubmit(adv, myForm, formLastName)
    {    

       if ((formLastName == 'blupageSearchForm'))    
       {  
           ns_7_B0ODECD418JK30II4MBA0M30I0_submitBluepages(adv, myForm);          
       }
       else if ((formLastName == 'otherSearchForm'))    
       {
           ns_7_B0ODECD418JK30II4MBA0M30I0_submitOthers(adv, myForm);
       }
    }
    
    
    function ns_7_B0ODECD418JK30II4MBA0M30I0_submitOthers(isAdvance, mySearchForm)
    {
        var theValue = "";

        for (var i = 0; i < mySearchForm.searchOption.length; i++) 
        {
            if (mySearchForm.searchOption[i].checked) 
            {
                theValue = mySearchForm.searchOption[i].value;
                var externalURL = false;
                var optionId = "";

                if ((theValue == "w3") && (isAdvance =="no")) 
                {
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[0];                
                    mySearchForm.qt.value = mySearchForm.others.value;              
                    mySearchForm.tabName.value = "w3";
                    mySearchForm.url.value = "";
                    mySearchForm.expandAdv.value = "no";
                    break;
                } 
                else if ((theValue == "w3") && (isAdvance =="yes")) 
                {
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[1];
                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "w3";
                    mySearchForm.expandAdv.value = "yes";
                    mySearchForm.url.value = "";
                    break;
                }
                else if ((theValue == "forums") && (isAdvance == "no")) 
                { 
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[0];                 
                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "forums";
                    mySearchForm.url.value = "";
                    mySearchForm.expandAdv.value = "no";
                    break;
                } 
                else if ((theValue == "forums") && (isAdvance == "yes")) 
                {
                    theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[1];
                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "forums";
                    mySearchForm.expandAdv.value = "yes";
                    mySearchForm.url.value = "";
                    break;
                } 
