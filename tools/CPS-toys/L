<script type="text/javascript">
function ns_7_JL13TL5510IJ60IAQFJAGE0087_isEmpty(myText) 
{
	var re = /^\s{1,}$/g; //match any white space including space, tab, form-feed, etc.

	if ((myText.value==null) || (myText.value.length==0) || ((myText.value.search(re)) > -1)) 
	{
		return true;
	}
	else 
	{
		return false;
	}
}


//----------------------------------------------------------//
//Validate the data entered
//----------------------------------------------------------//
function ns_7_JL13TL5510IJ60IAQFJAGE0087_ValidateLogin(myForm)
{
	if (ns_7_JL13TL5510IJ60IAQFJAGE0087_isEmpty(myForm.ns_7_JL13TL5510IJ60IAQFJAGE0087_LoginPortletFormID))
	{
		alert("You must fill in your intranet e-mail ID before submitting");
		myForm.ns_7_JL13TL5510IJ60IAQFJAGE0087_LoginPortletFormID.focus();
		return false;
	}
		
	if (ns_7_JL13TL5510IJ60IAQFJAGE0087_isEmpty(myForm.ns_7_JL13TL5510IJ60IAQFJAGE0087_LoginPortletFormPassword))
	{
		alert("You must fill in the password for your intranet e-mail ID before submitting");
		myForm.ns_7_JL13TL5510IJ60IAQFJAGE0087_LoginPortletFormPassword.focus();
		return false;
	}

	return true;
}

</script>

