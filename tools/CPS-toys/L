                    mySearchForm.qt.value = mySearchForm.others.value;
                    mySearchForm.tabName.value = "assets";
                    mySearchForm.expandAdv.value = "yes";
                    mySearchForm.url.value = "";
                    break;
                }  
                else if ((theValue == "tags") && (isAdvance == "no")) 
                { 
