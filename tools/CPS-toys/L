    function ns_7_B0ODECD418JK30II4MBA0M30I0_submitBluepages(isAdvance, mySearchForm)
    {
        var theValue = "";
        var searchTypeval = ns_7_B0ODECD418JK30II4MBA0M30I0_getSelectedValue(mySearchForm.searchBy);
        
        if(searchTypeval == "Entire profile") 
        {
           if(!ns_7_B0ODECD418JK30II4MBA0M30I0_checkWildCard(mySearchForm.searchFor,isAdvance)) 
           {
           	  return false;
           }
        }   
            
        if (searchTypeval == "-----------------------") 
        {
        	return false;
        }

        if( (ns_7_B0ODECD418JK30II4MBA0M30I0_checkMinSearch(mySearchForm.searchFor,2,isAdvance))) 
        {
        	mySearchForm.task.value = "viewrecord";
        
            if(isAdvance =="no")
            {
                 theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[2];
            }
            else
            {
                 if(searchTypeval == "Entire profile")
                 {
                     mySearchForm.task.value = "advanced_search_EP";
                     theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[4];
                 }
                 else
                 {  
                 	theValue = ns_7_B0ODECD418JK30II4MBA0M30I0_defaultURLs[3];
                 }
            }
            mySearchForm.action=theValue;     
            mySearchForm.submit();
            return true;
        }
        
        return false;
    }

    function ns_7_B0ODECD418JK30II4MBA0M30I0_checkMinSearch(searchObj, minNeeded, isAdvance)
    {
        var minSearchAlert = "You must enter at least 2 non-blank, non-asterisk characters in order to perform a search";        
        // count non-asterisks and non-blanks to ensure minimum chars needed
        var count=0;
        
        if (isAdvance =="yes") 
        {
        	return true;
        }

        for (var i=0;i<searchObj.value.length && count<=minNeeded;i++) 
        {
            thisChar = searchObj.value.substr(i,1);
            if (thisChar !="*" && thisChar !=" " ) 
            {
                count++;
            }
        }

        if (count < minNeeded)
        {
            alert(minSearchAlert);
            searchObj.focus();
            return false;
        }
        else
        {
            return true;
        }
    }

    function ns_7_B0ODECD418JK30II4MBA0M30I0_checkWildCard(searchObj, isAdvance)
    {
        var noWildCardAlert = "The wildcard character '*' is not supported for entire profile searches. Please refine your search without using wildcards.";
        if (isAdvance =="yes") 
        {
        	return true;
        }
        
        if (searchObj.value.indexOf("*")>-1) 
        {
            alert(noWildCardAlert);
            searchObj.focus();
            return false;
        } 
        else 
        {
            return true;
        }
    }

    function ns_7_B0ODECD418JK30II4MBA0M30I0_getSelectedValue(searchTypeObj)
    {
      var w = searchTypeObj.selectedIndex;
      var selected_text = searchTypeObj.options[w].text;
      return selected_text;
    }
    
	function ns_7_B0ODECD418JK30II4MBA0M30I0_checkForEnter(e, myForm, formLastName)
	{
		var keynum;

		if(window.event) // IE
		{
			keynum = e.keyCode;
		}
		else if(e.which) // Netscape/Firefox/Opera
		{
			keynum = e.which;
		}

		if (keynum == 13) 	// If enter was pressed
		{
			ns_7_B0ODECD418JK30II4MBA0M30I0_searchSubmit('no', myForm, formLastName);
			return false;
		}
		
		return true;
	}
    
